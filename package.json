{
  "name": "slack-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run schema:dev && next dev",
    "build": "npm run schema:prod && prisma generate && prisma db push && next build",
    "schema:dev": "copy prisma\\schema.dev.prisma prisma\\schema.prisma",
    "schema:prod": "copy prisma\\schema.prod.prisma prisma\\schema.prisma",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "npm run schema:prod && prisma generate",
    "db:push": "npm run schema:dev && prisma db push",
    "db:studio": "npm run schema:dev && prisma studio"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@prisma/client": "^6.16.2",
    "next": "15.5.3",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "5.9.2"
  }
}
